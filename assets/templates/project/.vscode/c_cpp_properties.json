{
    "env": {
        "IDF_PATH": ":IDF:PATH:",
        "MSYS32_PATH": ":MSYS32_PATH:",
        "XTENSA_PATH": ":XTENSA_PATH:"
    },
    "configurations": [
        {
            "name": "Win32",
            "intelliSenseMode": "gcc-x64",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "compilerPath": "\"{env:MSYS32_PATH}/opt/xtensa-esp32-elf/bin/xtensa-esp32-elf-gcc.exe\" -mlongcalls",
            "includePath": [
                "${env:MSYS32_PATH}/opt/xtensa-esp32-elf/**",
                "${workspaceFolder}/components/**",
                "${workspaceFolder}/build/**",
                "${workspaceFolder}/main",
                "${env:IDF_PATH}/components/**"
            ],
            "browse": {
                "path": [
                    "${env:MSYS32_PATH}/opt/xtensa-esp32-elf/**",
                    "${workspaceFolder}/components/**",
                    "${workspaceFolder}/build/**",
                    "${workspaceFolder}/main",
                    "${env:IDF_PATH}/components/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.c_cpp.db"
            }
        },
        {
            "name": "Linux",
            "intelliSenseMode": "gcc-x64",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "compilerPath": "\"{env:XTENSA_PATH}/bin/xtensa-esp32-elf-gcc.exe\" -mlongcalls",
            "includePath": [
                "${env:XTENSA_PATH}/**",
                "${workspaceFolder}/components/**",
                "${workspaceFolder}/build/**",
                "${workspaceFolder}/main",
                "${env:IDF_PATH}/components/**"
            ],
            "browse": {
                "path": [
                    "${env:XTENSA_PATH}/**",
                    "${workspaceFolder}/components/**",
                    "${workspaceFolder}/build/**",
                    "${workspaceFolder}/main",
                    "${env:IDF_PATH}/components/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.c_cpp.db"
            }
        }
    ],
    "version": 4
}