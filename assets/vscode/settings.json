{
    "terminal.integrated.shell.windows": "C:/msys32/usr/bin/bash.exe",
    "terminal.integrated.env.windows": {
        "CHERE_INVOKING": "1",
        "MSYSTEM": "MINGW32",
        "workspaceFolder": "${workspaceFolder}"
    },
    "terminal.integrated.shellArgs.windows": [
        "--login"
    ],
    "files.associations": {
        "*.cpp": "cpp",
        "ew": "cpp",
        "*.cc": "cpp",
        "*.C": "cpp",
        "*.cxx": "cpp",
        "*.c++": "cpp",
        "*.hpp": "cpp",
        "*.hh": "cpp",
        "*.H": "cpp",
        "*.hxx": "cpp",
        "*.h++": "cpp"
    },
    "C_Cpp.intelliSenseEngine": "Tag Parser"
}