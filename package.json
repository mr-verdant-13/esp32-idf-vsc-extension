{
	"preview": true,
	"name": "esp32-pm",
	"displayName": "ESP32 project manager",
	"description": "ESP32 project manager for Visual Studio Code. Setup, build, flash and monitor ESP32 applications.",
	"icon": "assets/local-data/icon.png",
	"version": "0.1.0",
	"publisher": "mrverdant13",
	"author": {
		"name": "Karlo Verde",
		"url": "https://www.linkedin.com/in/karlo-verde"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mrverdant13/esp32-pm-vsc-extension"
	},
	"homepage": "https://github.com/mrverdant13/esp32-pm-vsc-extension/blob/develop/README.md",
	"bugs": {
		"url": "https://github.com/mrverdant13/esp32-pm-vsc-extension/issues"
	},
	"keywords": [
		"ESP32",
		"ESP 32",
		"Espressif"
	],
	"extensionDependencies": [
		"ms-vscode.cpptools"
	],
	"activationEvents": [
		"*"
	],
	"os": [
		"win32"
	],
	"contributes": {
		"commands": [
			{
				"category": "ESP32-PM",
				"title": "Register ESP-IDF API",
				"command": "esp32-pm.register-esp-idf"
			},
			{
				"category": "ESP32-PM",
				"title": "Register Espressif Toolchain",
				"command": "esp32-pm.register-espressif-toolchain"
			},
			{
				"category": "ESP32-PM",
				"title": "Create project",
				"command": "esp32-pm.create-project"
			},
			{
				"category": "ESP32-PM",
				"title": "Initialize exisiting project",
				"command": "esp32-pm.init-project"
			},
			{
				"category": "ESP32-PM",
				"title": "Menuconfig",
				"command": "esp32-pm.menuconfig",
				"enablement": "workspaceFolderCount != 0"
			},
			{
				"category": "ESP32-PM",
				"title": "Defconfig",
				"command": "esp32-pm.defconfig"
			},
			{
				"category": "ESP32-PM",
				"title": "Build sub-project",
				"command": "esp32-pm.build-subproject",
				"enablement": "workspaceFolderCount != 0"
			},
			{
				"category": "ESP32-PM",
				"title": "Flash",
				"command": "esp32-pm.flash",
				"enablement": "workspaceFolderCount != 0"
			},
			{
				"category": "ESP32-PM",
				"title": "Monitor",
				"command": "esp32-pm.monitor",
				"enablement": "workspaceFolderCount != 0"
			},
			{
				"category": "ESP32-PM",
				"title": "Flash & Monitor",
				"command": "esp32-pm.flash-monitor",
				"enablement": "workspaceFolderCount != 0"
			},
			{
				"category": "ESP32-PM",
				"title": "Clean",
				"command": "esp32-pm.clean",
				"enablement": "workspaceFolderCount != 0"
			},
			{
				"category": "ESP32-PM",
				"title": "Remove auto-generated files",
				"command": "esp32-pm.remove-auto-gen",
				"enablement": "workspaceFolderCount != 0"
			}
		]
	},
	"main": "./out/extension.js",
	"engines": {
		"vscode": "^1.37.0"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"categories": [
		"Other"
	],
	"license": "SEE LICENSE IN LICENSE.txt"
}