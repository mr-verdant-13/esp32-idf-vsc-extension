{
	"name": "esp32-idf-vsc-extension",
	"version": "0.0.1",
	"publisher": "mr-verdant-13",
	"engines": {
		"vscode": "^1.37.0"
	},
	"displayName": "ESP32-IDF",
	"description": "Install the ESP-IDF, and build, flash and monitor ESP32 projects.",
	"categories": [
		"Other"
	],
	"preview": true,
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"category": "ESP32-IDF",
				"title": "Menuconfig",
				"command": "extension.menuconfig"
			},
			{
				"category": "ESP32-IDF",
				"title": "Defconfig",
				"command": "extension.defconfig"
			},
			{
				"category": "ESP32-IDF",
				"title": "Build test",
				"command": "extension.buildTest"
			},
			{
				"category": "ESP32-IDF",
				"title": "Flash",
				"command": "extension.flash"
			}
		]
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	}
}